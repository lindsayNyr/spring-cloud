<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.front.CocktailController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <HBox>
        <Button fx:id="ajouter" mnemonicParsing="false" onAction="#ajouter" text="Ajouter coktails"/>
        <Button fx:id="rechercher" mnemonicParsing="false" onAction="#rechercher" text="Rechercher cocktails"/>

    </HBox>
    <HBox>

        <Button fx:id="supprimer" mnemonicParsing="false" onAction="#supprimer" text="Supprimer cocktails"/>
        <Button fx:id="modifier" mnemonicParsing="false" onAction="#modifier" text="Modifier cocktails"/>

    </HBox>

</VBox>